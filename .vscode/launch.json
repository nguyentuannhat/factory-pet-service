{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "MES Domain",
        "runtimeExecutable": "yarn",
        "runtimeArgs": [
          "dev"
        ],
        "autoAttachChildProcesses": true,
        "restart": true,
        "sourceMaps": true,
        "stopOnEntry": false,
        "console": "integratedTerminal",
        "envFile": "${workspaceFolder}/env/.env",
      }
    ]
  }